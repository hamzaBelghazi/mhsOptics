<script src="/js/JeelizVTOWidget.js"></script>
<script src="/js/jeelizstart.js"></script>

<div class="try__on__overlay">
  <div class="overlay_try_on"></div>
  <div class="content try_on_container">
    <div id="JeelizVTOWidget">
      <canvas id="JeelizVTOWidgetCanvas"></canvas>
      <div class="JeelizVTOWidgetControls JeelizVTOWidgetControlsTop">
        <!-- ADJUST BUTTON:--><button id="JeelizVTOWidgetAdjust">
          <div class="buttonIcon"><i class="fas fa-arrows-alt"></i></div>
          Adjust</button
        ><!-- RESIZE WIDGET BUTTON:--><button
          id="buttonResizeCanvas"
          onclick="test_resizeCanvas();"
        >
          <div class="buttonIcon"><i class="fas fa-sync-alt"></i></div>
          Resize
        </button>
      </div>
      <!-- CHANGE MODEL BUTTONS:-->
      <div
        class="JeelizVTOWidgetControls"
        id="JeelizVTOWidgetChangeModelContainer"
      >
      <% if(product.virualsTrys) {%>
      <% if(product.virualsTrys[0]?.virtualsObject) {%>
       <% product.virualsTrys[0].virtualsObject.map((v)=> {  %>
        <button data-ver="<%= v %>" class="ver__btn"
          onclick="JEELIZVTOWIDGET.load_modelStandalone('/img/tryon/<%= v %>')"
        >
          Model
        </button>
        <% }) %> 
        <% } %> 
        <% } %> 
      </div>
      <!-- BEGIN ADJUST NOTICE-->
      <div id="JeelizVTOWidgetAdjustNotice">
        Move the glasses to adjust them. and scroll to resize them<button
          class="JeelizVTOWidgetBottomButton"
          id="JeelizVTOWidgetAdjustExit"
        >
          Quit
        </button>
      </div>
    </div>
    <a class="close_try_on" href="#">
      <span class="material-icons">close</span>
    </a>
  </div>
</div>
<!-- section identifer -->
<div class="prod_chain_nav">
  <div class="navigation">
    <a href="/"><%= __('home') %></a>
    <span class="siparetor">/</span>
    <a href="<%= `${product.glassesType === 'sunglasses' ? '/sunglasses' : "/prescription" }` %>" class="category__subtitle"><%= __('optical') %></a>
    <span class="siparetor">/</span>
    <p class="product__name">
      <%= product.title %>
      </p>
  </div>
</div>

<!-- product projrction -->

<section class="single__product__section section">
  <div class="product_head">
    <div class="product_important_info">
      <h3 class="product__name"><%= product.title %></h3>
      <p class="product__price">
        <%= product.price %> <span class="currency">$</span>
      </p>
    </div>
    <div class="cart__actions">
      <a
        href="#"
        class="add_to_cart add_fram_to_cart primary_button"
        data-id="<%= product.id %>"
        =""
        ><%= __('add_to_cart') %></a
      >
      <% if(product.glassesType == 'prescription') { %>
      <a href="/lense/<%= product.id %>" class="custom_lens secondary_button"><%= __('Customize_lens') %></a>
      <% } %>
    </div>
  </div>
  <div class="product_overview">
    <div class="product_previews">
      <div id="product-slider" class="product_slider">
        <div class="splide__arrows">
          <button class="splide__arrow splide__arrow--prev">
            <span class="material-icons"> arrow_left </span>
          </button>
          <button class="splide__arrow splide__arrow--next">
            <span class="material-icons"> arrow_right </span>
          </button>
        </div>
        <div class="splide__track">
          <ul class="splide__list">
            <% product.images.map(img => { %>
            <li class="splide__slide">
              <img src="/img/products/<%= img %>" />
            </li>
            <% }) %>
          </ul>
        </div>
      </div>

      <ul id="thumbnails" class="thumbnails">
        <% product.images.map(img => { %>
        <li class="thumbnail">
          <img src="/img/products/<%= img %>" />
        </li>
        <% }) %>
      </ul>
    </div>

    <div class="product_descriptions">
      <div class="product_proprerties">
        <div class="proprerties_title">
          <h3 class="title"><%= __("PROPERTIES") %></h3>
        </div>
        <div class="proprerties">
          <div class="gender_wraper">
            <span class="key"><%= __('Gender') %>:</span>
            <span class="value"><%= product.productGender %> </span>
          </div>
          <div class="chape_wraper">
            <span class="key"><%= __('Shape') %>:</span>
            <span class="value"><%= product.shape %> </span>
          </div>
          <div class="frame-color_wraper">
            <span class="key"><%= __('Frame_colour') %>:</span>
            <span class="value"><%= product.colors %> </span>
          </div>
          <div class="temple-color_wraper">
            <span class="key"><%= __('Temple_colour') %>:</span>
            <span class="value"><%= product.templeColor %> </span>
          </div>
          <div class="materials_wraper">
            <span class="key"><%= __("Frame material") %>:</span>
            <span class="value"><%= product.frameMatirial %> </span>
          </div>
        </div>
      </div>
    </div>
    <div class="product_info">
      <div class="product_description_wrapper">
        <% if(product.virualsTrys && product.virualsTrys[0]?.virtualsObject) { %>
        <span class="try__on__start_btn secondary_button"
          ><%= __('Try_on') %> <span class="material-icons"> videocam </span>
        </span>
        <% } %>
        <p class="product_description"><%= product.description %></p>
       
        </div>
