entHTML('beforeend', html);\n        spinnerhidd();\n      });\n    }\n  } catch (err) {\n    console.log(err);\n    spinnerhidd();\n  }\n};\n\n// pagination/loading\n\nexport const loadeMoreProd = async (pagenatObj, filter) => {\n  try {\n    spinnerLoader();\n\n    const res = await axios(\n      `/api/products?${filter}&limit=3&page=${pagenatObj}`\n    );\n\n    if (res.data.status === 'success') {\n      if (!productsContainer) return;\n\n      const { data } = res.data.data;\n\n      if (!data.length) {\n        spinnerhidd();\n        showAlert('error', 'no more resulte');\n        return;\n      }\n\n      data.map((p) => {\n        const html = `\n        <div class=\"product_cart\">\n              <img\n                src=\"/img/products/${p.images[0]}\"\n                alt=\"${p.title}\"\n                class=\"product_preview\"\n              />\n              <span class=\"product_title\">${p.title}</span>\n              <a href=\"/product/${p.id}\" class=\"product_actions\">\n                <span class=\"price\">${p.price}$</span>\n                <span class=\"try_on\">\n                  <span class=\"material-icons\"> videocam </span>\n                </span>\n              </a>\n            </div>\n        `;\n        productsContainer.insertAdjacentHTML('beforeend', html);\n        spinnerhidd();\n      });\n    }\n  } catch (err) {\n    console.log(err);\n    spinnerhidd();\n  }\n};\n"},"lineCount":null}},"error":null,"hash":"055e7df0163043d21b066f9487d2d101","cacheData":{"env":{}}}